{
  "name": "islandora/islandora-starter-site",
  "description": "Project template for Islandora sites.",
  "type": "project",
  "license": "GPL-2.0-or-later",
  "homepage": "https://www.islandora.ca",
  "support": {
    "docs": "https://islandora.github.io/documentation/",
    "chat": "https://www.islandora.ca/community#channels-of-communication"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    {
      "type": "package",
      "package": {
        "name": "library/pdf.js",
        "version": "2.16.105",
        "type": "drupal-library",
        "dist": {
          "url": "https://github.com/mozilla/pdf.js/releases/download/v2.16.105/pdfjs-2.16.105-dist.zip",
          "type": "zip"
        }
      }
    }
  ],
  "require": {
    "php": "^7.4 || ^8",
    "composer/installers": "^1.9",
    "cweagans/composer-patches": "^1.7",
    "drupal/admin_toolbar": "^3.1",
    "drupal/advanced_search": "^2.0.0@beta",
    "drupal/auto_entitylabel": "^3.2",
    "drupal/bartik": "^1.0",
    "drupal/better_exposed_filters": "^6.0",
    "drupal/bibcite": "^2.0@beta",
    "drupal/citation_select": "^1.0@beta",
    "drupal/coi": "^4.0",
    "drupal/config_update": "^2.0@alpha",
    "drupal/context": "^5@RC",
    "drupal/controlled_access_terms": "^2",
    "drupal/core-composer-scaffold": "^10.1",
    "drupal/core-recommended": "^10.1",
    "drupal/datafield": "^2.0",
    "drupal/entity_reference_exposed_filters": "^2.0.0-alpha2",
    "drupal/eva": "^3.1",
    "drupal/facets": "^2",
    "drupal/field_group": "^3",
    "drupal/field_permissions": "^1",
    "drupal/field_report": "^2.1",
    "drupal/flood_control": "^3.0",
    "drupal/flysystem": "^2.0@alpha",
    "drupal/hal": "^1.0||^2.0",
    "drupal/inline_entity_form": "^3.0@RC",
    "drupal/islandora": "^2.8.1",
    "drupal/islandora_mirador": "^2.3",
    "drupal/manage_display": "^3.0",
    "drupal/matomo": "^1.19",
    "drupal/metatag": "^2.0",
    "drupal/migrate_tools": "^6.0",
    "drupal/multilingual_menu_urls": "^10.0",
    "drupal/openid_connect": "^3.0@alpha",
    "drupal/openseadragon": "^2",
    "drupal/pdf": "^1.1",
    "drupal/recaptcha": "^3.4",
    "drupal/rest_oai_pmh": "^2.0@beta",
    "drupal/search_api_solr": "^4.2",
    "drupal/security_review": "^3.1",
    "drupal/simple_block": "^1.7",
    "drupal/simple_sitemap": "^4.2",
    "drupal/superfish": "^1.7",
    "drupal/taxonomy_manager": "^2.0",
    "drupal/term_merge": "^2.0@beta",
    "drupal/translatable_menu_link_uri": "^2.1",
    "drupal/twig_tweak": "^3.2",
    "drupal/views_data_export": "^1.2",
    "drupal/views_field_view": "^1.0@beta",
    "drush/drush": "^12.5",
    "islandora-rdm/islandora_fits": "dev-8.x-1.x as 1.x-dev",
    "islandora/islandora": "^2.8.1",
    "islandora/views_nested_details": "^1.0",
    "library/pdf.js": "^2.4",
    "mjordan/islandora_workbench_integration": "dev-main"
  },
  "conflict": {
    "drupal/drupal": "*",
    "drupal/core": "<=8"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "discard-changes": "stash",
    "allow-plugins": {
      "composer/installers": true,
      "cweagans/composer-patches": true,
      "drupal/core-composer-scaffold": true,
      "dealerdirect/phpcodesniffer-composer-installer": true
    },
    "sort-packages": true
  },
  "extra": {
    "drupal-scaffold": {
      "locations": {
        "web-root": "web/"
      },
      "file-mapping": {
        "[web-root]/robots.txt": {
          "append": "assets/robots-additions.txt"
        }
      }
    },
    "installer-paths": {
      "web/core": [
        "type:drupal-core"
      ],
      "web/libraries/{$name}": [
        "type:drupal-library"
      ],
      "web/modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "web/profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "web/themes/contrib/{$name}": [
        "type:drupal-theme"
      ],
      "drush/Commands/contrib/{$name}": [
        "type:drupal-drush"
      ],
      "web/modules/custom/{$name}": [
        "type:drupal-custom-module"
      ],
      "web/profiles/custom/{$name}": [
        "type:drupal-custom-profile"
      ],
      "web/themes/custom/{$name}": [
        "type:drupal-custom-theme"
      ]
    },
    "patches": {
      "drupal/core": {
        "Datafield error fix: display references in select list": "./patches/form.inc.patch",
        "Datafield error fix: save references in select list": "./patches/SqlContent.patch"
      }
    }
  },
  "scripts": {
    "post-root-package-install": [
      "Islandora\\StarterSite::rootPackageInstall"
    ]
  },
  "require-dev": {
    "drupal/config_inspector": "^2.1",
    "drupal/devel": "^5.0",
    "drupal/restui": "^1.21"
  },
  "autoload": {
    "classmap": [
      "assets/IslandoraStarterSite.php"
    ]
  }
}
